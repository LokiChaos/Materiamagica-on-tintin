#nop CLASS: datatypes;
#nop PROVIDES: lib/adt;

/* Queue Functions  */
#function {queue_push} {
	#list {%1} {insert} {-1} {%2};
	#return {#nop ;};
}

#function {queue_pop} {
	#if {&{%1[]}} {
		#var result {${%1[1]}};
		#list {%1} {delete} {1};
	};
}

#function {queue_peek} {
	#if {&{%1[]}} {
		#var result {${%1[1]}};
	};
}

#function {queue_purge} {
	#var {%1} {};
	#return {#nop ;};
}

#function {queue_delete} {
	#unvar {%1};
	#return {#nop ;};
}

#function {queue_depth} {
	#return &{%1[]};
}

